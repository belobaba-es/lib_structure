  "description": "Libreria de dominio e infraestructura para la estrcutura de cuentas segregadas",
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" ",
