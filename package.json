  "devDependencies": {
    "@types/uuid": "^9.0.2",
    "prettier": "^3.0.2"
  },
  "dependencies": {
    "aws-sdk": "^2.1438.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^6.9.0",
    "mongodb": "^5.7.0",
    "pino": "^8.15.0",
    "uuid": "^9.0.0"
